# Blockify Skill Environment Configuration
# Copy this file to .env and fill in your API keys

# Blockify API key - Get from: https://console.blockify.ai/
BLOCKIFY_API_KEY=

# OpenAI API key - Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Optional: Number of parallel workers for ingestion (default: 5)
BLOCKIFY_PARALLEL_WORKERS=10
